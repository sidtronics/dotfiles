
#Audio Control:

"pamixer -i 2" 
  XF86AudioRaiseVolume

"pamixer -d 2" 
  XF86AudioLowerVolume

"pamixer -t"
  XF86AudioMute

"pamixer --default-source -t"
  XF86AudioMicMute

#Backlight:

"brightnessctl -q -d intel_backlight s +2%"
  Alt + F4

"brightnessctl -q -d intel_backlight s 2%-"
  Alt + F3
